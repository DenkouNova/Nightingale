<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                  assembly="Nightingale"
                  namespace="Nightingale.Domain">

  <class name="Subsource" table="Subsource">
    <id name="Id">
      <generator class="increment" />
    </id>
    <property name="Name" />
    <property name="Disabled" />
    <many-to-one name="Source" class="Source" column="Source_Id"/>
    <set name="Links" cascade="all">
      <key>
        <column name="Subsource_Id"/>
      </key>
      <one-to-many class="Link"/>
    </set>
  </class>

</hibernate-mapping>